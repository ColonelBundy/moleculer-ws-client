    "@types/ws": "^4.0.1",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "ts-loader": "4.1.0",
    "webpack": "4.2.0",
    "webpack-cli": "2.0.13",
    "webpack-visualizer-plugin": "0.1.11"
  }
